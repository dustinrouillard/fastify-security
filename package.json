{
  "name": "@dustinrouillard/fastify-security",
  "version": "1.0.1",
  "description": "JWT/Fastify related security functions",
  "author": "Dustin Rouillard <code@dustin.sh>",
  "scripts": {
    "build": "tsc",
    "build:w": "tsc -w",
    "dev": "tsc -w",
    "push": "yarn build; cp -R package.json README.md LICENSE dist; (cd dist && yarn publish --non-interactive); rm -rf dist/package.json dist/README.md dist/LICENSE",
    "lnk": "yarn build; cp -R package.json README.md LICENSE dist; (cd dist && yarn link)"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^14.0.6",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@dustinrouillard/fastify-utilities": "^1.0.2",
    "fastify": "^2.14.1",
    "fastify-plugin": "^1.6.1",
    "jsonwebtoken": "^8.5.1"
  }
}
